div(ng-attr-title='{{attendee.fullmail}}')
  img.nopadding(src='/images/user.png', fallback-src='/images/user.png', ng-src='/api/avatars?objectType=user&email={{attendee.email|urlencode}}')
  span(ng-if='!readOnly')
    i.mdi.mdi-check-square-o(ng-show='attendee.clicked')
    i.mdi.mdi-crop-square(ng-show='!attendee.clicked')
