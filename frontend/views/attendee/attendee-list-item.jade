a.attendee-avatar(href="", ng-attr-title='{{attendee.fullmail}}')
  img(src='/images/user.png', fallback-src='/images/user.png', ng-src='/api/avatars?objectType=user&email={{attendee.email|urlencode}}')
  .checkbox(ng-if='!readOnly')
    label
      input(type="checkbox", ng-if="!attendee.clicked")
      input(type="checkbox", ng-if="attendee.clicked", checked)
      i.input-helper
